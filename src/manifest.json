{
  "name": "FHTWMP",
  "description": "This extension will host additional css and js for the FHTW moodle website!",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://moodle.technikum-wien.at/*"
      ],
      "js": ["fhtwmp.js"],
      "css": ["fhtwmp.css"]
    }
  ],
  "icons": {
    "16": "fhtwmp-logo-16.png",
    "48": "fhtwmp-logo-48.png",
    "128": "fhtwmp-logo-128.png"
  }
}